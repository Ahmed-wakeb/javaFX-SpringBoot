<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Line?>

<AnchorPane prefHeight="400" prefWidth="600" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="com.example.javaFXSpringBootDemo.RadarController">
    <Pane fx:id="radarPane" AnchorPane.bottomAnchor="40" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10">
        <Circle fx:id="radarScreen" centerX="200" centerY="200" radius="180" style="-fx-fill: #EFEFEF; -fx-stroke: #000000" />
        <Line fx:id="radarBeam" endX="200" endY="20" startX="200" startY="200" stroke="#FF0000" />
        <!-- Add other radar visuals, such as target markers or sweeps -->
    </Pane>
    <Button fx:id="startButton" onAction="#startButtonClicked" text="Start" AnchorPane.bottomAnchor="10" />
   <Button layoutX="504.0" layoutY="365.0" mnemonicParsing="false" text="Button" />
    <!-- Add other UI controls as needed -->
</AnchorPane>
